type Session {
  tokens: Tokens
  layout: LayoutPreferences!
  focusWorkspaceIndex: Int!
  workspaceFocusIndices: [WorkspaceFocus]!
}

type WorkspaceFocus {
  focusProjectIndex: Int!
  projectFocusIndices: [ProjectFocus]!
}

type ProjectFocus {
  focusWorkflowIndex: Int!
  focusDocumentIndex: Int!
}

input SessionInput {
  tokens: TokensInput
}
